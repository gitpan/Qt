OUTPUT
Qt_OBJECT
    $arg = objectify_ptr((void*)$var, MSTR($type));

PQt_OBJECT
    $arg = objectify_ptr((void*)$var, MSTR($type), 1);

VQt_OBJECT
    $arg = objectify_ptr((void*)$var, $var->setQtClassName(CLASS), 1);
    $var->setQtObject($arg);

INPUT
